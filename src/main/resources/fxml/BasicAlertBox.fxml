<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="136.0" prefWidth="333.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.BasicAlertBoxController">
   <children>
      <Label layoutX="14.0" layoutY="7.0" text="Error">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Label fx:id="errorLabel" layoutX="14.0" layoutY="38.0" prefHeight="30.0" prefWidth="306.0" text="An error occured. Sorry!" wrapText="true">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="okButton" layoutX="140.0" layoutY="96.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="55.0" text="Ok" />
   </children>
</AnchorPane>
